    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-status": "^1.4.2",
    "iptv-playlist-parser": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "prisma": "^2.19.0",
    "serialize-error": "^8.0.1",
    "tslint": "^6.1.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",

  "main": "build/src/index.js",
  "scripts": {
    "test": "exit 1",
    "start": "node build/src/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --no-notify ./src/index.ts",
    "build:watch": "tsc --watch",
    "lint": "tslint -p .",
